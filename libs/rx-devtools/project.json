    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/rx-devtools"],
      "options": {
        "jestConfig": "libs/rx-devtools/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
