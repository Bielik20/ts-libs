{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "react-demo",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "http-client": {
      "root": "libs/http-client",
      "sourceRoot": "libs/http-client/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/http-client",
            "buildableProjectDepsInPackageJsonType": "dependencies",
            "tsConfig": "libs/http-client/tsconfig.lib.json",
            "project": "libs/http-client/package.json",
            "entryFile": "libs/http-client/src/index.ts",
            "external": ["tslib"],
            "rollupConfig": "./tools/src/plugins/web-rollup-plugin",
            "assets": [
              {
                "glob": "libs/http-client/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/http-client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/http-client"],
          "options": {
            "jestConfig": "libs/http-client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@ns3/nx-npm:version"
        },
        "publish": {
          "executor": "@ns3/nx-npm:publish"
        }
      }
    },
    "react-demo": {
      "root": "apps/react-demo",
      "sourceRoot": "apps/react-demo",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/react-demo",
            "outputPath": "dist/apps/react-demo"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "react-demo:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "react-demo:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "react-demo:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/react-demo"],
          "options": {
            "jestConfig": "apps/react-demo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-demo/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "react-demo-e2e": {
      "root": "apps/react-demo-e2e",
      "sourceRoot": "apps/react-demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-demo-e2e/cypress.json",
            "tsConfig": "apps/react-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
