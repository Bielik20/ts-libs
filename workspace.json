{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "react-demo",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "css", "linter": "eslint" } }
  },
  "projects": {
    "react-demo": {
      "root": "apps/react-demo",
      "sourceRoot": "apps/react-demo",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/react-demo", "outputPath": "dist/apps/react-demo" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "react-demo:build", "dev": true },
          "configurations": {
            "production": { "buildTarget": "react-demo:build:production", "dev": false }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "react-demo:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/react-demo"],
          "options": { "jestConfig": "apps/react-demo/jest.config.js", "passWithNoTests": true }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/react-demo/**/*.{ts,tsx,js,jsx}"] }
        }
      }
    },
    "react-demo-e2e": {
      "root": "apps/react-demo-e2e",
      "sourceRoot": "apps/react-demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-demo-e2e/cypress.json",
            "tsConfig": "apps/react-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-demo:serve"
          },
          "configurations": { "production": { "devServerTarget": "react-demo:serve:production" } }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/react-demo-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "ts-utils": {
      "root": "libs/ts-utils",
      "sourceRoot": "libs/ts-utils/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/ts-utils",
            "tsConfig": "libs/ts-utils/tsconfig.lib.json",
            "project": "libs/ts-utils/package.json",
            "entryFile": "libs/ts-utils/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "./tools/src/plugins/react-rollup-plugin",
            "assets": [{ "glob": "libs/ts-utils/README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/ts-utils/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ts-utils"],
          "options": { "jestConfig": "libs/ts-utils/jest.config.js", "passWithNoTests": true }
        },
        "version": { "executor": "@ns3/nx-npm:version" },
        "publish": { "executor": "@ns3/nx-npm:publish" }
      }
    },
    "redux-entity": {
      "root": "libs/redux-entity",
      "sourceRoot": "libs/redux-entity/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/redux-entity",
            "tsConfig": "libs/redux-entity/tsconfig.lib.json",
            "project": "libs/redux-entity/package.json",
            "entryFile": "libs/redux-entity/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "./tools/src/plugins/react-rollup-plugin",
            "assets": [{ "glob": "libs/redux-entity/README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/redux-entity/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redux-entity"],
          "options": { "jestConfig": "libs/redux-entity/jest.config.js", "passWithNoTests": true }
        },
        "version": { "executor": "@ns3/nx-npm:version" },
        "publish": { "executor": "@ns3/nx-npm:publish" }
      }
    },
    "http-client": {
      "root": "libs/http-client",
      "sourceRoot": "libs/http-client/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/http-client",
            "tsConfig": "libs/http-client/tsconfig.lib.json",
            "project": "libs/http-client/package.json",
            "entryFile": "libs/http-client/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "./tools/src/plugins/react-rollup-plugin",
            "assets": [{ "glob": "libs/http-client/README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/http-client/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/http-client"],
          "options": { "jestConfig": "libs/http-client/jest.config.js", "passWithNoTests": true }
        },
        "version": { "executor": "@ns3/nx-npm:version" },
        "publish": { "executor": "@ns3/nx-npm:publish" }
      }
    },
    "react-di": {
      "root": "libs/react-di",
      "sourceRoot": "libs/react-di/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react-di",
            "tsConfig": "libs/react-di/tsconfig.lib.json",
            "project": "libs/react-di/package.json",
            "entryFile": "libs/react-di/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "./tools/src/plugins/react-rollup-plugin",
            "assets": [{ "glob": "libs/react-di/README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/react-di/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-di"],
          "options": { "jestConfig": "libs/react-di/jest.config.js", "passWithNoTests": true }
        },
        "version": { "executor": "@ns3/nx-npm:version" },
        "publish": { "executor": "@ns3/nx-npm:publish" }
      }
    },
    "rx-store": {
      "root": "libs/rx-store",
      "sourceRoot": "libs/rx-store/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/rx-store",
            "tsConfig": "libs/rx-store/tsconfig.lib.json",
            "project": "libs/rx-store/package.json",
            "entryFile": "libs/rx-store/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "./tools/src/plugins/react-rollup-plugin",
            "assets": [{ "glob": "libs/rx-store/README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/rx-store/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/rx-store"],
          "options": { "jestConfig": "libs/rx-store/jest.config.js", "passWithNoTests": true }
        },
        "version": { "executor": "@ns3/nx-npm:version" },
        "publish": { "executor": "@ns3/nx-npm:publish" }
      }
    }
  }
}
