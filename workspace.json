{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "generators": { "@nrwl/react": { "application": { "babel": true } } },
  "projects": {
    "ts-utils": {
      "root": "libs/ts-utils",
      "sourceRoot": "libs/ts-utils/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/ts-utils",
            "tsConfig": "libs/ts-utils/tsconfig.lib.json",
            "project": "libs/ts-utils/package.json",
            "entryFile": "libs/ts-utils/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "./tools/src/plugins/react-rollup-plugin",
            "assets": [{ "glob": "libs/ts-utils/README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/ts-utils/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ts-utils"],
          "options": { "jestConfig": "libs/ts-utils/jest.config.js", "passWithNoTests": true }
        },
        "version": { "executor": "@ns3/nx-npm:version" },
        "publish": { "executor": "@ns3/nx-npm:publish" }
      }
    }
  }
}
